
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>KOLAROO - landanimals</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="~/assets/img/favicon.png" rel="icon">
    <link href="~/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i"
          rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="~/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">


    <link href="~/assets/css/animals.css" rel="stylesheet" />

</head>


<body>

    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top ">
        <div class="container d-flex align-items-center justify-content-between">

            <a href="@Url.Action("kolaroo", "Home")" class="logo"><img src="~/assets/img/logo.png" alt="" class="img-fluid"></a>

            <nav id="navbar" class="navbar">
                <ul>
                    <li><a class="nav-link scrollto" href="@Url.Action("kolaroo", "Home")">Home</a></li>
                    <li><a class="nav-link scrollto active" href="@Url.Action("landanimals", "Home")">Land Animals</a></li>
                    <li><a class="nav-link scrollto" href="@Url.Action("habitat", "Home")">Habitats</a></li>
                </ul>
                <i class="bi bi-list mobile-nav-toggle"></i>
            </nav><!-- .navbar -->
        </div>
    </header><!-- End Header -->
    <!-- ======= Hero Section ======= -->
    <section id="hero">
        <div class="hero-container">
            <h1>KOLAROO VICTORIA</h1>
            <h2>Empowering people to save the Land Animals</h2>
            <a href="@Url.Action("kolaroo", "Home")" class="btn-get-started scrollto">Back to Home</a>
        </div>
    </section><!-- End Hero -->

    <main id="main">

        <!-- ======= Portfolio Section ======= -->
        <section id="portfolio" class="portfolio">
            <div class="container">

                <div class="section-title">
                    <h3><span>Meet Victoria's threatened species</span></h3>
                    <p>
                        Victoria is rich in unique Australian animals. However, a large and growing number of these animals are now threatened with extinction. Find out more about Victoria's endangered species, and their main threats.
                    </p>
                    <a href="@Url.Action("animal_vic_data", "threatened_animal_vic")" class="btn-get-started">Search From database</a>
                </div>

                <div class="row">
                    <div class="col-lg-12 d-flex justify-content-center">
                        <ul id="portfolio-flters">
                            <li data-filter="*" class="filter-active">All</li>
                            <li data-filter=".filter-1">BAWAWFROG</li>
                            <li data-filter=".filter-2">ROCKWALLABY</li>
                            <li data-filter=".filter-3"> BARREDBANDICOOT</li>
                            <li data-filter=".filter-4">GREETERGLIDER</li>
                            <li data-filter=".filter-5">LEADBEATER'S POSSUM</li>
                            <li data-filter=".filter-6">MOUNTAINPYGMY-POSSUM</li>
                        </ul>
                    </div>
                </div>

                <div class="row portfolio-container">

                    <div class="col-lg-4 col-md-6 portfolio-item filter-1">
                    
                                <img src="~/assets/img/portfolio/Baw Baw Frog2.jpg" class="img-fluid" alt="">
                                <div class="portfolio-info">
                                    <h4>Baw Baw Frog</h4>

                                    <a href="@Url.Action("BawBawFrog","Animals")" class="details-link" title="More Details"><i class="bx bx-link"></i></a>
                                </div>
                            
                    </div>

                    <div class="col-lg-4 col-md-6 portfolio-item filter-2">
                        <img src="~/assets/img/portfolio/Brush-tailed Rock-wallaby.jpg" class="img-fluid" alt="">
                        <div class="portfolio-info">
                            <h4>Brush-tailed Rock-wallaby</h4>
                     
                            <a href="@Url.Action("BrushtailedRock","Animals")" class="details-link" title="More Details"><i class="bx bx-link"></i></a>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6 portfolio-item filter-3">
                        <img src="~/assets/img/portfolio/Eastern Barred Bandicoot.jpg" class="img-fluid" alt="">
                        <div class="portfolio-info">
                            <h4>Eastern Barred Bandicoot</h4>
                          
                            <a href="@Url.Action("EasternBarredBandicoot","Animals")" class="details-link" title="More Details"><i class="bx bx-link"></i></a>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6 portfolio-item filter-4">
                        <img src="~/assets/img/portfolio/Greater Glider.jpg" class="img-fluid" alt="">
                        <div class="portfolio-info">
                            <h4>Greater Glider</h4>
                  
                            <a href="@Url.Action("GreaterGlider","Animals")" class="details-link" title="More Details"><i class="bx bx-link"></i></a>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6 portfolio-item filter-5">
                        <img src="~/assets/img/portfolio/Leadbeater's Possum.jpg" class="img-fluid" alt="">
                        <div class="portfolio-info">
                            <h4>Leadbeater's Possum</h4>
                            
                           
                            <a href="@Url.Action("LeadbeaterPossum","Animals")" class="details-link" title="More Details"><i class="bx bx-link"></i></a>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-6 portfolio-item filter-6">
                        <img src="~/assets/img/portfolio/Mountain Pygmy-possum.jpg" class="img-fluid" alt="">
                        <div class="portfolio-info">
                            <h4>Mountain Pygmy-possum</h4>
                        
                            <a href="@Url.Action("MountainPygmypossum","Animals")" class="details-link" title="More Details"><i class="bx bx-link"></i></a>
                        </div>
                    </div>


                </div>

            </div>
        </section><!-- End Portfolio Section -->


    </main><!-- End #main -->


    <a href="#" class="back-to-top d-flex align-items-center justify-content-center">
        <i class="bi bi-arrow-up-short"></i>
    </a>

    <!-- Vendor JS Files -->
    <script src="~/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/assets/js/main.js"></script>

</body>

</html>