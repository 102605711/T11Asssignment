                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="thumb-wrapper">
                                                    <div class="img-box">
                                                        <a href="@Url.Action("BrushtailedRock","Animal")">
                                                            <img src="~/portfolio/Brush-tailed Rock-wallaby2.jpg" class="img-fluid" alt="">
                                                    </div>
                                                    <div class="thumb-content">
                                                        <h4>Brush-tailed Rock-wallaby</h4>
                                                    </div>
                                                    </a>
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="thumb-wrapper">
                                                    <a href="@Url.Action("BawBawFrog","Animal")">
                                                        <div class="img-box">
                                                            <img src="~/portfolio/Baw Baw Frog2.jpg" class="img-fluid" alt="">
                                                        </div>
                                                        <div class="thumb-content">
                                                            <h4>Baw Baw Frog</h4>
                                                        </div>
                                                    </a>
                                                </div>
                                            </div>
                                            <div class="col-sm-3">
                                                <div class="thumb-wrapper">
                                                    <a href="@Url.Action("GreaterGlider","Animal")">
                                                        <div class="img-box">
                                                            <img src="~/portfolio/Greater Glider3.jpg" class="img-fluid" alt="">
                                                        </div>
                                                        <div class="thumb-content">
                                                            <h4>Greater Glider</h4>
                                                        </div>
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- Carousel controls -->
                                <a class="carousel-control-prev" href="#myCarousel" data-slide="prev">
                                    <i class="fa fa-angle-left"></i>
                                </a>
                                <a class="carousel-control-next" href="#myCarousel" data-slide="next">
                                    <i class="fa fa-angle-right"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </section><!-- End About Us Section -->
        <!-- ======= About Us Section ======= -->
